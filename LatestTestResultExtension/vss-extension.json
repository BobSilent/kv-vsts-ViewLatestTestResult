{
  "manifestVersion": 1,
  "id": "viewlatesttestresult",
  "version": "2.0.5",
  "name": "View Latest Test Result",
  "scopes": [ "vso.work", "vso.test" ],
  "description": "Allows you to easily view the latest test results for a test case from the work item form.",
  "publisher": "keesverhaar",
  "files": [
    {
        "path": "libs",
        "addressable": true
    },
    {
        "path": "src",
        "addressable": true
    },
    {
        "path": "marketplace",
        "addressable": true
    },
    {
      "path": "doc",
      "addressable": true
    }
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "Overview.md"
    }
  },
  "screenshots": [
    {
      "path": "doc/TestCaseForm.jpg"
    },
    {
      "path": "doc/ContextMenu.jpg"
    }
  ],
  "public": true,
  "icons": {
    "default": "marketplace/icon.png"
  },
  "tags": [
    "Test"
  ],
  "links": {
    "home": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "getstarted": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "learn": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "support": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult/issues"
    },
    "repository": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "issues": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult/issues"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "categories": [
    "Test"
  ],
  "contributions": [
    {  
        "id": "test-results-control",
        "type": "ms.vss-work-web.work-item-form-control",
        "description": "View latest test results for a test case on the work item form.",
        "targets": [
            "ms.vss-work-web.work-item-form"
        ],
        "properties": {
            "name": "Latest Test Results",
            "uri": "src/index.html",
            "height": 600
        }
    }

  ]
}
