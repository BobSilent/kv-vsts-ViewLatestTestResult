{
  "manifestVersion": 1,
  "id": "viewlatesttestresult",
  "version": "1.0.3",
  "name": "View Latest Test Result",
  "scopes": [ "vso.work", "vso.test" ],
  "description": "Allows you to easily view the latest test results for a test case from the work item form.",
  "publisher": "keesverhaar",
  "files": [
    {
        "path": "img",
        "addressable": true
    },
    {
        "path": "libs",
        "addressable": true
    },
    {
        "path": "src",
        "addressable": true
    },
    {
        "path": "marketplace",
        "addressable": true
    }
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "doc/Overview.md"
    }
  },
  "screenshots": [
    {
      "path": "doc/TestCaseForm.jpg"
    },
    {
      "path": "doc/ContextMenu.jpg"
    }
  ],
  "public": true,
  "icons": {
    "default": "marketplace/icon.png"
  },
  "tags": [
    "Test"
  ],
  "links": {
    "home": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "getstarted": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "learn": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "support": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult/issues"
    },
    "repository": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult"
    },
    "issues": {
      "uri": "https://github.com/KeesV/kv-vsts-ViewLatestTestResult/issues"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "categories": [
    "Test"
  ],
  "contributions": [
    {
      "id": "test-results-page",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "type": "ms.vss-work-web.work-item-form-page",
      "properties": {
        "name": "Test Results",
        "order": 100,
        "uri": "src/index.html"
      }
    }
  ]
}
